{
  "version": 2,
  "builds": [
    {
      "src": "api/debug-db.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/debug-db",
      "dest": "/api/debug-db.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/dist/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/app.js"
    }
  ]
}